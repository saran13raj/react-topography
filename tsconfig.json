{
	"compilerOptions": {
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"target": "ES2020",
		"module": "ESNext",
		"outDir": "./dist",
		"rootDir": "./",
		"declaration": true,
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}
